{
  "name": "astro-grab",
  "description": "Grab context for coding agents directly from your astro site.",
  "homepage": "https://astro-grab.nickradford.dev",
  "version": "0.1.6",
  "keywords": [
    "astro",
    "astro-integration",
    "withastro",
    "devtools",
    "overlay",
    "grab"
  ],
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "default": "./dist/client/index.js"
    },
    "./toolbar/app": {
      "types": "./dist/toolbar/app.d.ts",
      "default": "./dist/toolbar/app.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@astrojs/compiler": "^2.10.0"
  },
  "devDependencies": {
    "astro": "^5.0.0",
    "happy-dom": "^15.7.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0",
    "vite": "^6.0.0",
    "vitest": "^2.1.0"
  },
  "peerDependencies": {
    "astro": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
